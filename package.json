{
  "name": "vue-jdlstudio",
  "version": "0.1.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-jdlstudio --dest lib packages/index.js",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "packages/*",
    "lib/*",
    "src/*",
    "public/*",
    "examples/*",
    "*.json",
    "*.js"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "publishConfig": {
    "registry": "http://nexus.sjgs/repository/npm-hosted/"
  },
  "repository": {
    "type": "gitlab",
    "url": "git://gitlab.sjgs/jxw/vue-jdlstudio.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "codemirror": "^5.48.2",
    "core-js": "2.6.9",
    "dagre": "0.4.5",
    "element-ui": "^2.11.1",
    "file-saver": "^2.0.0",
    "jhipster-core": "3.6.10",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vue-bus": "^1.2.1",
    "vue-js-toolbar": "^1.0.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}